{
  "manifest_version": 3,
  "name": "PDF Form Autofill",
  "version": "1.0",
  "description": "An extension to auto-fill forms with data from PDF",
  "permissions": [
    "activeTab",
    "storage",
    "scripting"
  ],
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  }
}